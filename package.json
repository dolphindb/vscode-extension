{
    "name": "dolphindb-vscode",
    "type": "module",
    "version": "2.0.1210",
    "engines": {
        "vscode": ">=1.68.0"
    },
    "scripts": {
        "dev": "tsc --project ./tsconfig.node.json && node ./build.js",
        "build-server": "cd server && npm run build && cd .. && cp -rf server/build/* out/server/",
        "build": "tsc --project ./tsconfig.node.json && node ./build.js --production",
        "scan": "i18n-scan --input \"**/*.{ts,tsx}\"",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "fix": "eslint --fix \"**/*.{ts,tsx}\""
    },
    "devDependencies": {
        "@svgr/webpack": "^8.1.0",
        "@types/koa": "^2.15.0",
        "@types/node": "^20.12.12",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/vscode": "^1.89.0",
        "@types/ws": "^8.5.10",
        "css-loader": "^7.1.2",
        "eslint": "^9.3.0",
        "sass": "^1.77.2",
        "sass-loader": "^14.2.1",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.5",
        "webpack": "^5.91.0"
    },
    "dependencies": {
        "@ant-design/icons": "^5.3.7",
        "@ant-design/plots": "1.2.5",
        "@vscode/debugadapter": "^1.65.0",
        "@vscode/debugprotocol": "^1.65.0",
        "antd": "^5.17.4",
        "dayjs": "^1.11.11",
        "dolphindb": "^3.0.32",
        "koa": "^2.15.3",
        "lodash": "^4.17.21",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-object-model": "^1.2.5",
        "tslib": "^2.6.2",
        "vscode-languageclient": "^7.0.0",
        "ws": "^8.17.0",
        "xshell": "^1.0.112"
    },
    "pnpm": {
        "patchedDependencies": {
            "koa@2.15.3": "patches/koa@2.15.3.patch"
        }
    }
}