{
    "name": "dolphindb-vscode",
    "type": "module",
    "version": "3.0.210",
    "engines": {
        "vscode": ">=1.68.0"
    },
    "scripts": {
        "dev": "node --experimental-transform-types ./dev.ts",
        "build": "node --experimental-transform-types ./build.ts && rollup --config",
        "dev-languageserver": "rollup --config --watch",
        "build-languageserver": "rollup --config",
        "scan": "i18n-scan --input \"**/*.{ts,tsx}\"",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "fix": "eslint --fix \"**/*.{ts,tsx}\""
    },
    "devDependencies": {
        "vscode-languageserver": "^9.0.1",
        "vscode-languageserver-textdocument": "^1.0.11",
        "@svgr/webpack": "^8.1.0",
        "@types/koa": "^2.15.0",
        "@types/node": "^22.10.5",
        "@types/react": "^19.0.3",
        "@types/react-dom": "^19.0.2",
        "@types/vscode": "^1.96.0",
        "@types/ws": "^8.5.13",
        "eslint": "^9.17.0",
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "@ant-design/icons": "^5.5.2",
        "@ant-design/plots": "1.2.5",
        "@vscode/debugadapter": "^1.68.0",
        "@vscode/debugprotocol": "^1.68.0",
        "antd": "^5.23.0",
        "dayjs": "^1.11.13",
        "dolphindb": "^3.0.213",
        "koa": "^2.15.3",
        "lodash": "^4.17.21",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-object-model": "^1.2.21",
        "tslib": "^2.8.1",
        "vscode-languageclient": "^9.0.1",
        "ws": "^8.18.0",
        "xshell": "^1.1.22",
        "@rollup/plugin-commonjs": "^23.0.2",
        "@rollup/plugin-json": "^5.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-typescript": "^9.0.2",
        "rimraf": "^4.1.2",
        "rollup": "^3.3.0",
        "rollup-plugin-dts": "^5.0.0",
        "rollup-plugin-typescript2": "^0.34.1"
    },
    "pnpm": {
        "patchedDependencies": {
            "koa@2.15.3": "patches/koa@2.15.3.patch"
        }
    }
}
