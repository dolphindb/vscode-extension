        "@ant-design/plots": "1.2.5",
        "@vscode/debugadapter": "^1.65.0",
        "@vscode/debugprotocol": "^1.65.0",
        "antd": "^5.18.3",
        "dayjs": "^1.11.11",
        "dolphindb": "^3.0.101",
        "koa": "^2.15.3",
        "lodash": "^4.17.21",
